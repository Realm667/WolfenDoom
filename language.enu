HINTTEXTC2M1A = "\cU<crouch> \cCand avoid light to draw less attention.";
HINTTEXTC2M1B = "\cUIf you are running low on health and ammo, look for safe paths.";
HINTTEXTC2M1C = "\cU<crouch> \cCand one-hit-stab enemies from behind to save ammo.";
HINTTEXTC2M1D = "\cCAvoid the watchtowers. You can't kill them all.";
HINTTEXTC2M1E = "\cCTry to stay out of the light to draw less attention.";
HINTTEXTC2M1F = "\cCStay away from windows so scientists don't see you.";

MAPMESSAGES01 = "\cCThe door can't be opened from this side, \nthere is no way out this way!";
